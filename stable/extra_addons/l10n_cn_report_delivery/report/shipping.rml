  <story>
    <para style="P15">[[repeatIn(objects,'o')]]</para>
    <para style="P15">[[ (not(o.sale_id) and o.type== 'out') and 'Error, no sale order attached to this picking !' or removeParentNode('para') ]]</para>
    <para style="P1">Delivery Order/交货单 : [[ o.name ]]</para>
    <blockTable colWidths="257.0,2.0,257.0" repeatRows="1" style="Tableau1">
      <tr>
        <td>
          <para style="P13">Shipping address/发货地址：</para>
          <para style="P15"><font face="Helvetica">[[ setLang('zh_CN') ]]</font>[[ o.partner_id.address_id and o.address_id.partner_id and o.address_id.partner_id.name or '' ]] (<font face="Helvetica">[[ setLang('en_US') ]][[ o.address_id and o.address_id.partner_id and o.address_id.partner_id.name or '' ]])[[ setLang(o.partner_id.lang) ]]</font></para>
          <para style="P15">[[ o.address_id and o.address_id.street or '']]</para>
          <para style="P15">[[ o.address_id and o.address_id.street2 or removeParentNode('para')]]</para>
          <para style="P15">[[ o.address_id and o.address_id.zip or '']] [[ o.address_id and o.address_id.city or '']]</para>
          <para style="P15">[[ (o.address_id and o.address_id.state_id and o.address_id.state_id.name) or removeParentNode('para') ]] - [[ o.address_id and o.address_id.country_id and o.address_id.country_id.name or '']]</para>
          <para style="P15">Tel/电话：[[ o.address_id and o.address_id.phone or removeParentNode('para')]]</para>
          <para style="P4"><font face="Helvetica">Recipient/</font><font face="Helvetica" size="9.0">收货员:</font> [[ (o.address_id and o.address_id.name) or '' ]] </para>
          <para style="P5">
            <font color="white"> </font>
          </para>
        </td>
        <td>
          <para style="P14">
            <font color="white"> </font>
          </para>
        </td>
        <td>
          <para style="P13">Invoicing address/发票地址：</para>
          <para style="P6">
            <font color="white"> </font>
          </para>
          <para style="P6">[[ o.sale_id and o.sale_id.partner_invoice_id and o.sale_id.partner_invoice_id.street or '']]</para>
          <para style="P6">[[ (o.sale_id and o.sale_id.partner_invoice_id and o.sale_id.partner_invoice_id.street2) or removeParentNode('para')]]</para>
          <para style="P6">[[ o.sale_id and o.sale_id.partner_invoice_id and o.sale_id.partner_invoice_id.zip or '']] [[ o.sale_id and o.sale_id.partner_invoice_id and o.sale_id.partner_invoice_id.city or '' ]]</para>
          <para style="P7">[[ (o.sale_id and o.sale_id.partner_invoice_id and o.sale_id.partner_invoice_id.state_id and o.sale_id.partner_invoice_id.state_id.name) or removeParentNode('para') ]] - [[ o.sale_id and o.sale_id.partner_invoice_id and o.sale_id.partner_invoice_id.country_id and o.sale_id.partner_invoice_id.country_id.name or '']]</para>
        </td>
      </tr>
    </blockTable>
    <para style="P3">
      <font color="white"> </font>
    </para>
    <blockTable colWidths="74.0,74.0,74.0,74.0,74.0,74.0,74.0" style="Table1">
      <tr>
        <td>
          <para style="P8">Order Ref.</para>
          <para style="P8">订单号</para>
        </td>
        <td>
          <para style="P8">Customer PO#</para>
          <para style="P2">客户参考</para>
        </td>
        <td>
          <para style="P8">Order Date</para>
          <para style="P8">订单日期</para>
        </td>
        <td>
          <para style="P8">Shipping Date</para>
          <para style="P8">发货日期</para>
        </td>
        <td>
          <para style="P8">Carrier</para>
          <para style="P8">承运公司</para>
        </td>
        <td>
          <para style="P8">Tracking Ref.</para>
          <para style="P8">承运公司单号</para>
        </td>
        <td>
          <para style="P8">Weight</para>
          <para style="P8">重量</para>
        </td>
      </tr>
    </blockTable>
    <blockTable colWidths="74.0,74.0,74.0,74.0,74.0,74.0,74.0" style="Table2">
      <tr>
        <td>
          <para style="P17">[[ o.sale_id and o.sale_id.name ]]</para>
        </td>
        <td>
          <para style="P17">[[ o.sale_id and o.sale_id.client_order_ref]]</para>
        </td>
        <td>
          <para style="P17">[[ formatLang(o.date,date_time=True) ]]</para>
        </td>
        <td>
          <para style="P17">[[ formatLang(o.min_date,date_time = True) ]]</para>
        </td>
        <td>
          <para style="P17">[[ o.carrier_id and o.carrier_id.name or '' ]]</para>
        </td>
        <td>
          <para style="P17">[[ o.carrier_tracking_ref or '' ]]</para>
        </td>
        <td>
          <para style="P17">[[ formatLang(o.weight) ]]</para>
        </td>
      </tr>
    </blockTable>
    <blockTable colWidths="518.0" style="Table5">
      <tr>
        <td>
          <para style="P16">[[ format(o.note) ]]</para>
        </td>
      </tr>
    </blockTable>
    <blockTable colWidths="350.0,85.0,82.0" repeatRows="1" style="Table3">
      <tr>
        <td>
          <para style="P9">[Code] Description</para>
          <para style="P18">[编号] 说明</para>
        </td>
        <td>
          <para style="P9">Lot</para>
          <para style="P9">生产批次</para>
        </td>
        <td>
          <para style="P9">Quantity</para>
          <para style="P9">数量</para>
        </td>
      </tr>
    </blockTable>
    <section>
      <para style="P10">[[ repeatIn([line for line in o.move_lines if (line.state == 'confirmed' or line.state=='done' or line.state=='assigned')],'line') ]]</para>
      <blockTable colWidths="350.0,85.0,82.0" style="Table4">
        <tr>
          <td>
            <para style="P15">[[get_product_desc(line)]]</para>
            <para style="P15">
              <font color="white"> </font>
            </para>
            <para style="P15">[[get_partner_ref(line.partner_id.id, line.product_id.id) or removeParentNode('para')]]</para>
          </td>
          <td>
            <para style="P12">[[ (line.prodlot_id and (line.prodlot_id.name + (line.prodlot_id.ref and ('/' + line.prodlot_id.ref) or ''))) or ' ' ]]</para>
          </td>
          <td>
            <para style="P11">[[ formatLang(line.product_qty, digits=get_digits(dp='Product UoM')) ]] [[ line.product_uom and line.product_uom.name ]]</para>
          </td>
        </tr>
      </blockTable>
      <para style="P16">
        <font color="white"> </font>
      </para>
    </section>
